<form method="post" action="{{ path('app_offre_delete', {'id': offre.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette candidature ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ offre.id) }}">
    <button class="btn btn-danger mt-5">Supression</button>
</form>
